{
  "name": "@publish-kit/react-dnd-treeview",
  "version": "0.0.1-alpha.1",
  "description": "A draggable / droppable React-based treeview component.",
  "author": "Minoru Okuyama <okuyama@analogic.jp> (https://github.com/minop1205/)",
  "contributors": [
    {
      "name": "Han",
      "email": "han@deskbtm.com"
    }
  ],
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": {
      "default": "./src/*.ts"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "url": "https://github.com/deskbtm/publish",
    "directory": "first_party/bfs",
    "type": "git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc",
    "prepack": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "typescript": "5.6.2"
  },
  "dependencies": {
    "@juggle/resize-observer": "3.4.0",
    "dnd-multi-backend": "8.0.3",
    "framer-motion": "6.5.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dnd-touch-backend": "16.0.1",
    "react-use-measure": "2.1.1"
  }
}
